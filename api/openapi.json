{
  "openapi": "3.1.0",
  "info": {
    "title": "ISS Current Location",
    "description": "Provides the current location of the International Space Station (including latitude, longitude, and timestamp) and a list of people currently in space. Users are advised to poll the API no more than once every 5 seconds.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://api.open-notify.org/"
    }
  ],
  "paths": {
    "/iss-now.json": {
      "get": {
        "summary": "Get the current location of the International Space Station",
        "operationId": "GetIssLocation",
        "responses": {
          "200": {
            "$ref": "#/components/responses/ISSLocationResponse"
          }
        }
      }
    },
    "/astros.json": {
      "get": {
        "summary": "Get a list of astronauts currently in space",
        "operationId": "GetPeopleInSpace",
        "responses": {
          "200": {
            "$ref": "#/components/responses/PeopleInSpaceResponse"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ISSLocation": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "integer",
            "example": 1734874602
          },
          "iss_position": {
            "$ref": "#/components/schemas/ISSPosition"
          },
          "message": {
            "type": "string",
            "example": "success"
          }
        },
        "required": [
          "timestamp",
          "iss_position",
          "message"
        ],
        "example": {
          "timestamp": 1734874602,
          "iss_position": {
            "longitude": "2.6217",
            "latitude": "-19.5888"
          },
          "message": "success"
        }
      },
      "ISSPosition": {
        "type": "object",
        "properties": {
          "longitude": {
            "type": "string",
            "example": "2.6217"
          },
          "latitude": {
            "type": "string",
            "example": "-19.5888"
          }
        },
        "required": [
          "longitude",
          "latitude"
        ],
        "example": {
          "longitude": "2.6217",
          "latitude": "-19.5888"
        }
      },
      "PeopleInSpace": {
        "type": "object",
        "properties": {
          "people": {
            "$ref": "#/components/schemas/Astronauts"
          },
          "number": {
            "type": "integer",
            "example": 12
          },
          "message": {
            "type": "string",
            "example": "success"
          }
        },
        "required": [
          "people",
          "number",
          "message"
        ],
        "example": {
          "people": [
            {
              "craft": "ISS",
              "name": "Oleg Kononenko"
            },
            {
              "craft": "ISS",
              "name": "Nikolai Chub"
            },
            {
              "craft": "ISS",
              "name": "Tracy Caldwell Dyson"
            },
            {
