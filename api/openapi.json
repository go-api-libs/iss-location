            },
            {
              "craft": "Tiangong",
              "name": "Li Cong"
            },
            {
              "craft": "Tiangong",
              "name": "Ye Guangfu"
            }
          ],
          "number": 12,
          "message": "success"
        }
      },
      "Astronauts": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/Astronaut"
        },
        "example": [
          {
            "craft": "ISS",
            "name": "Oleg Kononenko"
          },
          {
            "craft": "ISS",
            "name": "Nikolai Chub"
          },
