      "Astronaut": {
        "type": "object",
        "properties": {
          "craft": {
            "type": "string",
            "example": "ISS"
          },
          "name": {
            "type": "string",
            "example": "Oleg Kononenko"
          }
        },
        "required": [
          "craft",
          "name"
        ],
        "example": {
          "craft": "ISS",
          "name": "Oleg Kononenko"
        }
      }
    },
    "responses": {
      "ISSLocationResponse": {
        "description": "Returns the current location of the International Space Station",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/ISSLocation"
            },
            "example": {
              "timestamp": 1734874602,
              "iss_position": {
                "longitude": "2.6217",
                "latitude": "-19.5888"
              },
              "message": "success"
            }
          }
        }
      },
      "PeopleInSpaceResponse": {
